{
  "name": "camino-backend",
  "version": "1.0.0",
  "description": "Camino Travel Platform - Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "node --max-old-space-size=384 src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:api": "jest --runInBand",
    "seed": "node src/database/seeders/runSeeders.js",
    "fix-passwords": "node src/database/fixAdminPassword.js",
    "migrate": "node src/database/migrations/runMigrations.js",
    "migrate:languages": "node database/add_language_fields_safe.js",
    "clear-and-reseed": "node database/clear_and_reseed.js"
  },
  "keywords": [
    "travel",
    "api",
    "backend",
    "express",
    "mysql"
  ],
  "author": "Camino Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "nodemailer": "^6.9.7",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}

